<script>

    //提交添加
    $("#addRobotBtn").on('click', function () {

        var date = new Date();
        var parmets = {
            add_name: $("#supplier_name").val(),
            add_by: $("#supplier_user").val(),
            add_time: date.getFullYear() + '-' + (date.getMonth() + 1) + '-' + date.getDate() + ' ' + date.getHours() + ':' + date.getMinutes(),
            add_time_number: (date.getTime() / 1000).toFixed()
        }

        console.log(parmets)
        if (parmets.add_name == '') {
            layer.tips('请填写供应商', '#supplier_name')
        } else {
            console.log(parmets)
            $.ajax({
                url: 'doAddSupplier',
                type: 'POST',
                data: parmets,
                success: function (retmsgurndata) {
                    console.log(retmsgurndata)
                    $("#addForm").hide();
                    $("#supplier").append("<option value='" + $("#supplier_name").val() + "'>" + $("#supplier_name").val() + "</option>");
                    $("#supplier_name").val('');
                    $("#add_by").val('');
                    layer.closeAll();
                    layer.msg('添加成功');
                }, error: function (returndata) {
                    alert('添加失败，请检查是否有非法词汇！');
                }
            });
        }
    });

    //添加用户显示层
    $("#addSupplierBtn").on('click', function () {
        $('#addForm').removeClass('hidden');
        $('#addsupplier').show();
        layer.open({
            type: 1,
            skin: 'layui-layer-rim',
            shade: false,
            title: false, //不显示标题
            content: $('#addForm'), //捕获的元素
            area: ['420px', "350px"],
            cancel: function (index) {
                layer.close(index);
            },
            scrollbar: false
        });
    });
    function uploadFile() {
        var file = document.getElementById("file")
        var formData = new FormData();
        console.log(file)
        console.log(file.files)
        if (file.files.length > 8) {
            layer.alert('上传图片大于8张')
        } else {
            for (var i = 0; i < file.files.length; i++) {
                if (file.files[i].type != 'image/png' && file.files[i].type != 'image/jpeg') {
                    layer.alert('上传图片格式有误')
                    return false
                } else if (file.files[i].size / 1024 > 2000) {
                    layer.alert('上传图片大小不能超过2M')
                    return false
                } else {
                    formData.append('file', file.files[i]);
                }

                $('.limit_pic').text(8 - file.files.length)
            }

            $.ajax({
                url: '/admin/uploadSingle',
                type: 'POST',
                data: formData,
                async: false,
                cache: false,
                contentType: false,
                processData: false,
                success: function (data) {
                    var data = data.data;
                    console.log(data)
                    if (data.length != 0) {
                        data.forEach(function (item, index) {
                            item = item.replace('public', '');
                            var li = '<li class="' + index + '"><img src="' + item + '"><span class="delete"></span></li><span class="exchange ' + index + '"></span>';
                            $('#file-upload').append(li);
                        })
                    }
                    $('.delete').click(function () {
                        var exchange = $(this).parents('li').attr('class')
                        $('.' + exchange).remove();
                        $('.limit_pic').text(parseInt($('.limit_pic').text()) + 1)
                    })
                    $('.exchange').click(function () {
                        if ($(this).next().length && $(this).prev().length)
                            var prev = $(this).prev().html();
                        var next = $(this).next().html();
                        $(this).prev().html(next);
                        $(this).next().html(prev);
                    })
                },
                error: function () {
                    layer.msg('上传文件格式错误')
                }
            });
        }


    }
</script>
