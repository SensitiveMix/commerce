                    $('#top-nav').css('width', width + 100);
                }

                $.ajax({
                    type: 'GET',
                    url: '/getHeadBanner',
                    data: {},
                    dataType: 'json',
                    success: function (data) {
                        $('#kv-img').attr('src', data.image_url);
                    }
                });
            })
