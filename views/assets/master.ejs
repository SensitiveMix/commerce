<script>
    $(function () {
        $('#menu-master').on('mouseover',function () {
            $('.submenu').show();
            $('.submenu').css('position','absolute');
            $('#zlight-nav').zlightMenu();
        })
        $('.nav-top').on('mouseleave',function () {
            $('#slider-menu').hide();
            $('.submenu').hide();
        })
    })
</script>
