                                    <!--span class="glyphicon glyphicon-chevron-left"></span-->
                                </a>
                                <a class="right carousel-control" href="#bs-carousel" data-slide="next">
                                    <i class="fa fa-angle-right"></i>
                                    <!--span class="glyphicon glyphicon-chevron-right"></span-->
                                </a>
                            </div>
                        </section>
                    </div>
                </div>
            </div>
