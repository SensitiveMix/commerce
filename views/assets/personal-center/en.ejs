                                <script>
                                    function address_submit() {
                                        if ($('#address').val() == '') {
                                            layer.tip('Please enter area', '#address')
                                        } else if ($('#detail_address').val() == '') {
                                            layer.tip('Please enter detail address', '#detail_address')
                                        }
                                        $.ajax({
                                            type: 'POST',
                                            url: '/change-address',
                                            data: {
                                                name: $('#old_name').val(),
                                                areaCode: $('#address').val(),
                                                detail_address: $('#detail_address').val()
                                            },
                                            dataType: 'json',
                                            success: function (data) {
                                                if (data.code == '200') {
                                                    $('#areaCode').val(data.account.areaCode)
                                                    $('#detail_address').val(data.account.detail_address)
                                                    layer.msg('CHANGE SUCCESS')
                                                } else if (data.code == '500') {
                                                    layer.msg('CHANGE FAILED,PASSWORD INCORRECT')
                                                }
                                            },
                                            error: function (err) {
                                                if (err == '500') {
                                                    layer.msg('NETWORK ERROR');
                                                }
                                            }
                                        });
                                    }
                                    function profile_submit() {
                                        if ($('#new_nick_name').val() == '') {
                                            layer.msg('Please Enter New Name')
                                        } else {
                                            $.ajax({
                                                type: 'POST',
                                                url: '/change-profile',
                                                data: {
                                                    gender: $('#gender').find("option:selected").val(),
                                                    origin_nick_name: $('#origin_nick_name').val(),
                                                    new_nick_name: $('#new_nick_name').val(),
                                                    describes: $('#describes').val(),
                                                    customers_company: $('#customers_company').val()
                                                },
                                                dataType: 'json',
                                                success: function (data) {
                                                    if (data.code == '200') {
                                                        $('#new_nick_name').val('')
                                                        $('#origin_nick_name').val(data.account.nick_name)
                                                        $('#customers_company').val(data.account.company)
                                                        $('#gender').find("option:selected").val(data.account.sex)
                                                        layer.msg('CHANGE SUCCESS')
                                                    } else if (data.code == '500') {
                                                        $('#new_nick_name').val('')
                                                        $('#existing_password').val('')
                                                        layer.msg('CHANGE FAILED,PASSWORD INCORRECT')
                                                    }
                                                },
                                                error: function (err) {
                                                    if (err == '500') {
                                                        $('#new_nick_name').val('')
                                                        $('#existing_password').val('')
                                                        layer.msg('NETWORK ERROR');
                                                    }
                                                }
                                            });
                                        }

                                    }


                                    function email_submit() {
                                        $.ajax({
                                            type: 'POST',
                                            url: '/change-email',
                                            data: {
                                                old_name: $('#old_name').val(),
                                                existing_password: $('#existing_password').val(),
                                                newEmail: $('#login-email-address1').val()
                                            },
                                            dataType: 'json',
                                            success: function (data) {
                                                if (data.code == '200') {
                                                    $('#existing_password').val('')
