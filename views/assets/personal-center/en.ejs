                                                    layer.msg('CHANGE SUCCESS')
                                                } else if (data.code == '500') {
                                                    $('#existing_password').val('')
                                                    layer.msg('CHANGE FAILED,PASSWORD INCORRECT')
                                                }
                                            },
                                            error: function (err) {
                                                if (err == '500') {
                                                    $('#existing_password').val('')
                                                    layer.msg('NETWORK ERROR');
                                                }
                                            }
