                                                type: 'POST',
                                                url: '/change-password',
                                                data: {
                                                    old_name: $('#old_name').val(),
                                                    existing_password_1: $('#existing_password_1').val(),
                                                    login_password_one: $('#login-password1').val(),
                                                    login_password_twice: $('#login_password_twice').val()
                                                },
                                                dataType: 'json',
                                                success: function (data) {
