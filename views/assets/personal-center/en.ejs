                                        } else if ($('#existing_password_1').val() == '') {
                                            layer.msg('Please Enter Old Password')
                                        } else if ($('#login-password1').val() == '' || $('#login_password_twice').val() == '') {
                                            layer.msg('Please Enter New Password')
                                        } else {
