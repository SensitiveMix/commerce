                <script>
                    (function layerswitch() {
                        var switches = $(".boxswitch div");
                        switches.click(function () {
                            $(this).addClass('on').siblings().removeClass('on');
                            $('.contentBlock>div:eq(' + switches.index(this) + ')').show().siblings().hide();
                        });
                    })();

                    $('#profileForm').submit(function () {
                        var items = {
                            firstname: [{type: 'null', errMsg: litb.langs.poAddr.noFirstName}, {
                                type: 'latinMinlength',
                                minlength: 2,
                                errMsg: litb.langs.poAddr.shortFirstName.stringFormat(2)
                            }],
                            lastname: [{type: 'null', errMsg: litb.langs.poAddr.noLastName}, {
                                type: 'latinMinlength',
                                minlength: 2,
                                errMsg: litb.langs.poAddr.shortLastName.stringFormat(2)
                            }, {
                                type: 'fullname',
                                latinMaxlength: 34,
                                jpMaxlength: 11,
                                errMsg: litb.langs.poAddr.exceedFullName
                            }]
                        };
                        return $(this).formCheck(items, {
                            errinfoFinder: function (obj) {
                                return obj.next();
                            },
                            rules: {
                                latinMinlength: function (obj, checks) {
                                    if (litb.lan == 'ja' || litb.lan == 'ko')return true;
                                    return $(obj).val().length >= checks.minlength;
                                },
                                fullname: function (obj, checks) {
                                    var maxlength = (litb.lan == 'ja' || litb.lan == 'ko') ? checks.jpMaxlength : checks.latinMaxlength;
                                    return $('#firstname').val().length + $('#lastname').val().length <= maxlength;
                                }
                            }
                        });
                    });

                    $('#emailForm').submit(function () {
                        var items = {
                            email: [{type: 'null', errMsg: litb.langs.ckReg.noEmailAddress}, {
                                type: 'email',
                                errMsg: litb.langs.ckReg.invalidEmailAddress
                            }],
                            existing_password: [{
                                type: 'null',
                                errMsg: litb.langs.account.noExistPassword
                            }, {
                                type: 'minlength',
                                minlength: 5,
                                errMsg: litb.langs.ckReg.invalidPassword.stringFormat('5')
                            }]
                        };
                        return $(this).formCheck(items, {
                            errinfoFinder: function (obj) {
                                return obj.next();
                            }
                        });
                    });

                    $('#passwordForm').submit(function () {
                        var items = {
                            existing_password_1: [{
                                type: 'null',
                                errMsg: litb.langs.account.noExistPassword
                            }, {
                                type: 'minlength',
                                minlength: 5,
                                errMsg: litb.langs.ckReg.invalidPassword.stringFormat('5')
                            }],
                            password: [{type: 'null', errMsg: litb.langs.account.noNewPassword}, {
                                type: 'patternCheck',
                                errMsg: litb.langs.ckReg.registerInvalidPassword.stringFormat('7')
                            }],
                            confirmation: [{
                                type: 'null',
                                errMsg: litb.langs.account.reEnterNewPassword
                            }, {type: 'matchPassword', errMsg: litb.langs.ckReg.passwordNotMatch}]
                        };
                        return $(this).formCheck(items, {
                            errinfoFinder: function (obj) {
                                return obj.next();
                            },
                            rules: {
                                patternCheck: function (obj, checks) {
                                    return $(obj).val() != '' && $(obj).val().match(/\d/) && $(obj).val().match(/[a-zA-Z]/) && $(obj).val().length >= 7;
                                },
                                matchPassword: function (obj, checks) {
                                    return $(obj).val() == $('#login-password1').val();
                                }
                            }
                        });
                    });

                    $("#login-email-address1").autocompleteEmail({width: 10});
